Package: eNFI
Title: eLearning National Forest Inventory
Version: 0.0.0.9000
Authors@R: 
        c(person(
        given  = "Gael", 
        family = "Sola", 
        email  = c("gael.sola@fao.org", "gaelsola@hotmail.fr"), 
        role   = c("aut", "cre")
      )
